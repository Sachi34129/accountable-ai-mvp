    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/pdf-parse": "^1.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
