    "@napi-rs/canvas": "^0.1.79",
    "@prisma/client": "^5.7.1",
    "bullmq": "^5.1.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.10.38",
    "prisma": "^5.7.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
