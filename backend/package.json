{
  "name": "accountable-ai-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "mkdir -p logs && tsx watch src/server.ts",
    "build": "tsc",
    "start": "mkdir -p logs && node dist/server.js",
    "railway:start": "mkdir -p logs && prisma migrate deploy && node dist/server.js",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "worker": "mkdir -p logs && tsx src/workers/extractor.worker.ts",
    "worker:categorization": "mkdir -p logs && tsx src/workers/categorization.worker.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
